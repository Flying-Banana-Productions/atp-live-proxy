{
  "name": "atp-live-proxy",
  "version": "1.0.0",
  "description": "A proxy server for ATP tennis live API with caching capabilities",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:large": "node --max-old-space-size=4096 src/server.js",
    "start:xlarge": "node --max-old-space-size=8192 src/server.js",
    "dev": "nodemon src/server.js --ignore 'logs/*'",
    "dev:large": "nodemon --exec 'node --max-old-space-size=4096' src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "atp",
    "tennis",
    "proxy",
    "api",
    "cache"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "json-diff-ts": "^4.8.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "redis": "^5.8.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
