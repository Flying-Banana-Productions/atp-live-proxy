# Server Configuration
PORT=3000
NODE_ENV=development

# ATP API Configuration
ATP_API_BASE_URL=https://api.protennislive.com/feeds
ATP_BEARER_TOKEN=your_tournament_bearer_token_here

# Cache Configuration
